    {
      "name": "01. Livros",
      "description": "CRUD Principal. Requer IDs válidos de Autor, Editora e Categoria.",
      "item": [
        {
          "name": "GET | Listar Todos os Livros",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/livros",
              "host": ["{{baseUrl}}"],
              "path": ["api", "livros"]
            }
          }
        },
        {
          "name": "GET | Buscar Livro por ID",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/livros/{{livroId}}",
              "host": ["{{baseUrl}}"],
              "path": ["api", "livros", "{{livroId}}"]
            }
          }
        },
        {
          "name": "POST | Criar Novo Livro",
          "description": "Exemplo de criação. **idAutor, idEditora, idCategoria** devem ser ObjectIds válidos.",
          "request": {
            "method": "POST",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"titulo\": \"O Senhor dos Anéis: A Sociedade do Anel\",\n    \"isbn\": \"978-8575003260\",\n    \"anoPublicacao\": 1954,\n    \"paginas\": 576,\n    \"estoque\": 5,\n    \"idAutor\": \"{{autorId}}\",\n    \"idEditora\": \"{{editoraId}}\",\n    \"idCategoria\": \"{{categoriaId}}\",\n    \"tags\": [\"fantasia\", \"épico\"]\n}",
              "options": { "raw": { "language": "json" } }
            }
          }
        },
        {
          "name": "PUT | Atualizar Livro (Parcial)",
          "description": "Atualiza o estoque e tags (atualização parcial).",
          "request": {
            "method": "PUT",
            "header": [{ "key": "Content-Type", "value": "application/json" }],
            "body": {
              "mode": "raw",
              "raw": "{\n    \"estoque\": 8,\n    \"tags\": [\"fantasia\", \"épico\", \"clássico\"]\n}",
              "options": { "raw": { "language": "json" } }
            },
            "url": {
              "raw": "{{baseUrl}}/api/livros/{{livroId}}",
              "host": ["{{baseUrl}}"],
              "path": ["api", "livros", "{{livroId}}"]
            }
          }
        },
        {
          "name": "DELETE | Excluir Livro",
          "request": {
            "method": "DELETE",
            "url": {
              "raw": "{{baseUrl}}/api/livros/{{livroId}}",
              "host": ["{{baseUrl}}"],
              "path": ["api", "livros", "{{livroId}}"]
            }
          }
        }
